@model List<Input>

<table class="table table-hover">

  <thead>

    <tr>
      <th scope="col">ID</th>
      <th scope="col">Product ID</th>
      <th scope="col">Count</th>
      <th scope="col">Time</th>
    </tr>
  </thead>

  <tbody>
    
      @foreach(var item in Model)
      {
        <tr class="table-secondary">
          <th scope="row">@item.ID</th>
          <td>@item.ProductID</td>
          <td>@item.Count</td>
          <td>@item.Time</td>
        </tr>
      }
        
  </tbody>

</table>

